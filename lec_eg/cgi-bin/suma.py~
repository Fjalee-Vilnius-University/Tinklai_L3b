#!/usr/bin/env python
import sys

n1s, n2s = sys.argv[1].split(' ')

n1 = n1s.split('=')[1]
n2 = n2s.split('=')[1]


print("Content-type:text/html\r\n\r\n")
print("<html>")
print("<head>")
print('<title>Paprasta CGI programa</title> <meta charset="UTF-8">')
print("</head>")
print("<body>")
print("<h2>Skaičių  %s ir  %s suma %s</h2>"  % (n1, n2, str(int(n1)+int(n2))))
print("</body>")
print("</html>")

